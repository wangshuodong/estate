<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="DB2Tables" targetRuntime="MyBatis3">
        <commentGenerator>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <!-- 数据库链接URL、用户名、密码 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/estate" userId="root" password="Passw0rd">
        </jdbcConnection>
        <!-- 默认false，把JDBC DECIMAL 和 NUMERIC 类型解析为 Integer，为 true时把JDBC DECIMAL 和
            NUMERIC 类型解析为java.math.BigDecimal -->
        <javaTypeResolver>
            <property name="forceBigDfactoryPicecimals" value="false"/>
        </javaTypeResolver>
        <!-- 生成模型的包名和位置 -->
        <javaModelGenerator targetPackage="com.wangsd.web.model" targetProject=".\src\main\java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <!-- 生成的映射文件包名和位置 -->
        <sqlMapGenerator targetPackage="com.wangsd.web.mapping" targetProject=".\src\main\java">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        <!-- 生成DAO的包名和位置 -->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.wangsd.web.dao.basic" targetProject=".\src\main\java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>
        <!-- 要生成那些表(更改tableName和domainObjectName就可以)-->
        <!--
        <table tableName="menu" domainObjectName="Menu"/>
        <table tableName="users" domainObjectName="Users"/>
        <table tableName="housing" domainObjectName="Housing"/>
        <table tableName="role_menu" domainObjectName="RoleMenu"/>
        
        <table tableName="printinfo" domainObjectName="Printinfo"/>
        <table tableName="property" domainObjectName="Property"/>
        
        <table tableName="serviceinfo" domainObjectName="Serviceinfo"/>
        
        <table tableName="role" domainObjectName="Role">
            <generatedKey column="id" sqlStatement="MySql" identity="true"/>
        </table>
        <table tableName="permission" domainObjectName="Permission"/>
        <table tableName="role_permission" domainObjectName="RolePermission"/>
        <table tableName="billaccount" domainObjectName="Billaccount"/>
        <table tableName="roominfo" domainObjectName="Roominfo"/>
        <table tableName="department" domainObjectName="Department">
        	<generatedKey column="id" sqlStatement="MySql" identity="true"/>
        </table>
        -->
        <table tableName="users" domainObjectName="Users"/>
    </context>
</generatorConfiguration>